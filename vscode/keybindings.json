// 将键绑定放入此文件中以覆盖默认值
// 将键绑定放入此文件中以覆盖默认值
[{
        "key": "f12",
        "command": "extension.open"
    },
    {
        "key": "ctrl+alt+o",
        "command": "-extension.open"
    },
    {
        "key": "ctrl+shift+v",
        "command": "editor.action.clipboardPasteAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+h",
        "command": "editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+8",
        "command": "workbench.action.toggleEditorGroupLayout"
    },
    {
        "key": "shift+alt+1",
        "command": "-workbench.action.toggleEditorGroupLayout"
    },
    {
        "key": "shift+alt+1",
        "command": "workbench.action.closeEditorsInOtherGroups"
    },
    {
        "key": "ctrl+f12",
        "command": "markdown.showPreviewToSide",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k v",
        "command": "-markdown.showPreviewToSide",
        "when": "editorFocus"
    },
    {
        "key": "alt+up",
        "command": "editor.emmet.action.incrementNumberByOneTenth",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+up",
        "command": "editor.emmet.action.incrementNumberByOne",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+up",
        "command": "editor.emmet.action.incrementNumberByTen",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+down",
        "command": "editor.emmet.action.decrementNumberByOneTenth",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+down",
        "command": "editor.emmet.action.decrementNumberByOne",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+down",
        "command": "editor.emmet.action.decrementNumberByTen",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+p alt+a",
        "command": "projectManager.saveProject"
    },
    {
        "key": "ctrl+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "shift+alt+f",
        "command": "-editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+f",
        "command": "extension.relativePath",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+h",
        "command": "-extension.relativePath",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+h",
        "command": "beautify.format"
    },
    {
        "key": "ctrl+f2",
        "command": "bookmarks.toggle",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+k",
        "command": "-bookmarks.toggle",
        "when": "editorTextFocus"
    },
    {
        "key": "f2",
        "command": "bookmarks.jumpToNext",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+l",
        "command": "-bookmarks.jumpToNext",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+f2",
        "command": "bookmarks.jumpToPrevious",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+j",
        "command": "-bookmarks.jumpToPrevious",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+f2",
        "command": "bookmarks.clearFromFile"
    },
    { // tab 不选择系统提示(一般提示是 系统提示 emmet提示) , 在配置里面先设置"emmet.triggerExpansionOnTab": true  ,  tab按照emmet语法展开 , 甚至有些css如tac ta-l这些都没有出现在提示中 , 但是还是会自动按照emmet补全 ; enter保持按照提示顺序选择提示
        "key": "tab",
        "command": "-acceptSelectedSuggestion",
        "when": "editorTextFocus && suggestWidgetVisible"
    }
]